# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index

GET     /abc                         controllers.ProductsController.r3

GET     /xyz                         controllers.ProductsController.boaster
GET     /mnp                         controllers.ProductsController.ora
GET     /jkl                         controllers.ProductsController.video
GET     /mno                         controllers.ServiceController.support
GET     /pqr                         controllers.ServiceController.training
GET     /rst                         controllers.ServiceController.consulting
GET     /bg                          controllers.CommunityController.blog
GET     /pg                          controllers.CommunityController.projects
GET     /tp                          controllers.CommunityController.templates
GET     /ab                         controllers.CompanyController.aboutUs
GET     /ot                          controllers.CompanyController.ourTeam
GET     /cr                          controllers.CompanyController.careers
GET     /ct                          controllers.CompanyController.contactUs




# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
